(SETQ MTEST '(

	(CALL 0 ADR)
	(CALL 1 CNT)
	(CALL 2 DATA)

START		(PUSHJ 	INIT)
WRITE-UP	(IWRITE ADR ADR)
		(DEC CNT)
		(BPL WRITE-UP)
		(PUSHJ	INIT)
READ-POS	(READ	DATA ADR)
		(NOR	DATA DATA)
		(WRITE	DATA ADR)
		(ADD	DATA ADR)
		(BMC	LOOSE-POS)
		(INC	ADR)
		(DEC	CNT)
		(BPL	READ-POS)
		(PUSHJ	INIT)
READ-NEG	(READ	DATA ADR)
		(ADD	DATA ADR)
		(BMC	LOOSE-NEG)
		(INC	ADR)
		(DEC	CNT)
		(BPL	READ-NEG)
		(JUMP	START)
INIT		(LOD 0 ADR)
		(LOD 7777 CNT)
		(POPJ)
LOOSE-NEG	(ERROR	& LOST ON COMPLEMENT OF ADDR)
LOOSE-POS	(ERROR	& LOST ON ADDR)
))