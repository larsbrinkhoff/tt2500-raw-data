(SETQ  LOADER '(

	(CALL 0 WD)
	(CALL 1 ADR)
	(CALL 2 CNT)
	(CALL 3 CHK)

START		(GET 	WD IN 	& RESET TTY)
WAIT-FLAG	(PUSHJ 	CHR-IN)
		(GET	WD WD 	& TO SEE IT)
		(BPL	WAIT-FLAG & WAIT FOR FLAG)
		(INC	WD)
		(BMI	WAIT-FLAG)
		(SUB	CHK CHK)
		(PUSHJ	GET-WD)
		(GET 	XR WD  & START ADDRESS)
		(PUSHJ	GET-WD)
		(GET 	CNT WD & NUMBER OF WORDS)
LOAD-LOOP	(PUSHJ	GET-WD)
		(LCM	WD)
		(DEC	CNT)
		(BPL	LOAD-LOOP)
;		(PUSHJ	GET-WD)
;		(GET	CHK CHK & LOOK AT CHECK SUM)
;		(BNE	LOOSE)
		(JUMP	0)
GET-WD		(PUSHJ	CHR-IN)
		(PUSHJ	CHR-IN)
		(PUSHJ	CHR-IN)
		(PUSHJ	CHR-IN)
;		(ADD	CHK WD)
		(POPJ)
CHR-IN		(DIS	INT2 1 0)
		(JUMP	CHR-IN)
		(GET	5 IN)
		(andi wd wd 7777)
		(rot wd wd 14)
		(ANDI	5 5 17)
		(IOR	WD 5)
		(POPJ)
;LOOSE		(ERROR 	& CHECKSUM LOST)
