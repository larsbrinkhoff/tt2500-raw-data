(SETQ MTEST '(

	(CALL 0 ADR)
	(CALL 1 CNT)
	(CALL 2 DATA)
	(CALL 3 LOOK)

START		(PUSHJ 	INIT)
WRITE-UP	(IWRITE ADR ADR)
		(DEC CNT)
		(BPL WRITE-UP)
		(PUSHJ	INIT)
READ-POS	(READ	ADR LOOK)
		(GET	DATA LOOK)
		(NOR	DATA DATA)
		(WRITE	ADR DATA)
		(ADD	DATA ADR)
		(PUT	ADR YCOR)
		(PUT	DATA XCOR)
;		(BMC	LOOSE-POS)
		(INC	ADR)
		(DEC	CNT)
		(BPL	READ-POS)
		(PUSHJ	INIT)
READ-NEG	(READ	ADR LOOK)
		(GET	DATA LOOK)
		(ADD	DATA ADR)
;		(BMC	LOOSE-NEG)
		(INC	ADR)
		(DEC	CNT)
		(BPL	READ-NEG)
		(JUMP	START)
INIT		(LOD	ADR )
		(0)
		(LOD	CNT )
		(7777)
		(POPJ)
LOOSE-NEG	(ERROR	& LOST ON COMPLEMENT OF ADDR)
LOOSE-POS	(ERROR	& LOST ON ADDR)
